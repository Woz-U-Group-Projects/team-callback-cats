token\");\r\n                setCookie(null, \"token\", token, {});\r\n                axiosAuth.post('/current-user', {})\r\n                .then(res => {\r\n                    console.log('RES =====>', res)\r\n                    dispatch({\r\n                        type: 'LOGIN',\r\n                        payload: res.data,\r\n                    });\r\n                });\r\n            }\r\n        });\r\n    }, []);\r\n\r\n    return <>{children}</>;\r\n};\r\n\r\nexport default FirebaseAuthState;"]},"metadata":{},"sourceType":"module"}